{
  "@context": "https://linkedsoftwaredependencies.org/bundles/npm/@rubeneschauzier/sparql-query-parameter-instantiator/^2.0.0/components/context.jsonld",
  "@id": "urn:sparql-query-parameter-instantiator:default",
  "@type": "QuerySequenceInstantiator",
  "count": 20,
  "seed": 0,
  "sequenceGenerator": {
    "@type": "SequenceGenerator",
    "meanLogSequenceLength": 3,
    "stdLogSequenceLength": 0.2,
    "meanLogTransitionProbability": -2,
    "stdLogTransitionProbability": 0.25,
    "meanLogSessionLength": 1.7,
    "stdLogSessionLength": 0.5,
    "refinementPatternProbability": 0.1,
    "temperature": 0.1,
    "findNextInstantiationValue": {
      "@type": "QueryNextInstantiatorValue",
      "dataLocations": [
        "out-snb/social_network/social_network_activity_0_0.ttl",
        "out-snb/social_network/social_network_person_0_0.ttl",
        "out-snb/social_network/social_network_static_0_0.ttl"
      ],
      "termMappingTransformer": {
        "@type": "ValueTransformerCsvMap",
        "file": "out-fragments/transformed-activities.csv"
      },
      "transformers": [
        {
          "@type": "TermTransformerBiDirectional",
          "originalRegex": "^http://www.ldbc.eu",
          "originalString": "http://www.ldbc.eu",
          "fragmentedRegex": "^http://localhost:3000/www.ldbc.eu",
          "fragmentedString": "http://localhost:3000/www.ldbc.eu"
        },
        {
          "@type": "TermTransformerBiDirectional",
          "originalRegex": "^http://dbpedia.org",
          "originalString": "http://dbpedia.org",
          "fragmentedRegex": "^http://localhost:3000/dbpedia.org",
          "fragmentedString": "http://localhost:3000/dbpedia.org"
        },
        {
          "@type": "TermTransformerBiDirectional",
          "originalRegex": "^http://www.w3.org/2002/07/owl",
          "originalString": "http://www.w3.org/2002/07/owl",
          "fragmentedRegex": "^http://localhost:3000/www.w3.org/2002/07/owl",
          "fragmentedString": "http://localhost:3000/www.w3.org/2002/07/owl"
        },
        {
          "@type": "TermTransformerBiDirectional",
          "originalRegex": "^http://www.ldbc.eu/ldbc_socialnet/1.0/data/pers([0-9]*)$",
          "originalString": "http://www.ldbc.eu/ldbc_socialnet/1.0/data/pers$1",
          "fragmentedRegex": "^http://solidbench-server:3000/pods/([0-9]*)/profile/card#me$",
          "fragmentedString": "http://solidbench-server:3000/pods/$1/profile/card#me"
        }      
      ],
      "timeout": 30
    }
  },
  "destinationFilePath": "out-queries/",
  "personProvider": {
    "@type": "VariableTemplateNamedNode",
    "name": "person",
    "substitutionProvider": {
      "@id": "urn:sparql-query-parameter-instantiator:substitutionProviderPerson",
      "@type": "SubstitutionProviderCsv",
      "csvFilePath": "out-fragments/parameters-persons.csv",
      "columnName": "subject"
    }
  },
  "providers": [
    {
      "@type": "QuerySequenceTemplateProvider",
      "name": "interactive-discover-1",
      "queryTask": "Messages Person",
      "nextTemplates": [
        "interactive-short-5"
      ],
      "instantiationVariableTypeMap": {
        "person": "person"
      },
      "outputVariableTypeMap": {
        "message": "message"
      },      
      "nextTemplateProbabilities": [
        1
      ],
      "baseProbabilityTemplate": 0.25,
      "minRefinementLength": 2,
      "maxRefinementLength": 5,
      "maxLogits": 5,
      "templateFilePath": {
        "@id": "urn:variables:query-templates:interactive-discover-1.sparql",
        "@type": "Variable"
      },
      "refinementPatternsFilePath": {
        "@id": "urn:variables:query-refinements:interactive-discover-1.json",
        "@type": "Variable"
      },
      "variables": [
        {
          "@type": "VariableTemplateNamedNode",
          "name": "person",
          "valueTransformers": [
            {
              "@id": "urn:sparql-query-parameter-instantiator:valueTransformerCsvMapActivities",
              "@type": "ValueTransformerCsvMap",
              "file": "out-fragments/transformed-activities.csv"
            }
          ],
          "substitutionProvider": {
            "@id": "urn:sparql-query-parameter-instantiator:substitutionProviderPersonProbability",
            "@type": "SubstitutionProviderCsvSimilarityBasedProbability",
            "csvFilePath": "out-fragments/parameters-persons.csv",
            "csvFilePathSimilarities": "out-enhanced/parameters-similarities-people.csv",
            "columnName": "subject",
            "columnNameSimilaritySubject": "person",
            "separator": ";"
          }
        },
        {
          "@type": "VariableTemplateTimestamp",
          "name": "timestamp",
          "substitutionProvider": {
            "@id": "urn:sparql-query-parameter-instantiator:substitutionProviderTimestamps",
            "@type": "SubstitutionProviderCsv",
            "csvFilePath": "out-snb/substitution_parameters/interactive_9_param.txt",
            "separator": "|",
            "columnName": "maxDate"
          }
        }
      ],
      "iriTransformer": {
        "@id": "urn:sparql-query-parameter-instantiator:valueTransformerIri",
        "@type": "ValueTransformerIri",
        "searchRegex": "http://localhost:3000",
        "replacementString": "http://solidbench-server:3000"
      }
    },
    {
      "@type": "QuerySequenceTemplateProvider",
      "name": "interactive-discover-2",
      "queryTask": "Messages Person",
      "nextTemplates": [
        "interactive-short-5"
      ],
      "nextTemplateProbabilities": [
        1
      ],
      "instantiationVariableTypeMap": {
        "person": "person"
      },
      "outputVariableTypeMap": {
        "message": "message"
      },      
      "baseProbabilityTemplate": 0.25,
      "minRefinementLength": 2,
      "maxRefinementLength": 5,
      "maxLogits": 5,
      "templateFilePath": {
        "@id": "urn:variables:query-templates:interactive-discover-2.sparql",
        "@type": "Variable"
      },
      "refinementPatternsFilePath": {
        "@id": "urn:variables:query-refinements:interactive-discover-2.json",
        "@type": "Variable"
      },

      "variables": [
        {
          "@type": "VariableTemplateNamedNode",
          "name": "person",
          "valueTransformers": [
            {
              "@id": "urn:sparql-query-parameter-instantiator:valueTransformerCsvMapActivities"
            }
          ],
          "substitutionProvider": {
            "@id": "urn:sparql-query-parameter-instantiator:substitutionProviderPersonProbability"
          }
        },
        {
          "@type": "VariableTemplateTimestamp",
          "name": "maxDate",
          "substitutionProvider": {
            "@id": "urn:sparql-query-parameter-instantiator:substitutionProviderTimestamps"
          }
        }
      ],
      "iriTransformer": {
        "@id": "urn:sparql-query-parameter-instantiator:valueTransformerIri"
      }
    },
    {
      "@type": "QuerySequenceTemplateProvider",
      "name": "interactive-discover-8",
      "queryTask": "Messages Person",
      "nextTemplates": [
        "interactive-short-5"
      ],
      "nextTemplateProbabilities": [
        1
      ],
      "instantiationVariableTypeMap": {
        "person": "person"
      },
      "outputVariableTypeMap": {
        "message": "message"
      },      
      "baseProbabilityTemplate": 0.25,
      "minRefinementLength": 2,
      "maxRefinementLength": 5,
      "maxLogits": 5,
      "templateFilePath": {
        "@id": "urn:variables:query-templates:interactive-discover-8.sparql",
        "@type": "Variable"
      },
      "refinementPatternsFilePath": {
        "@id": "urn:variables:query-refinements:interactive-discover-8.json",
        "@type": "Variable"
      },
      "variables": [
        {
          "@type": "VariableTemplateNamedNode",
          "name": "person",
          "valueTransformers": [
            {
              "@id": "urn:sparql-query-parameter-instantiator:valueTransformerCsvMapActivities"
            }
          ],
          "substitutionProvider": {
            "@id": "urn:sparql-query-parameter-instantiator:substitutionProviderPersonProbability"
          }
        },
        {
          "@type": "VariableTemplateNamedNode",
          "name": "tagParam",
          "substitutionProvider": {
            "@id": "urn:sparql-query-parameter-instantiator:substitutionProviderTag",
            "@type": "SubstitutionProviderCsv",
            "csvFilePath": "out-fragments/parameters-tags.csv",
            "columnName": "object",
            "uniqueValues": true
          }
        }
      ],
      "iriTransformer": {
        "@id": "urn:sparql-query-parameter-instantiator:valueTransformerIri"
      }
    },
    {
      "@type": "QuerySequenceTemplateProvider",
      "name": "interactive-short-5",
      "queryTask": "Messages",
      "nextTemplates": [
        "interactive-discover-1",
        "interactive-discover-2"
      ],
      "nextTemplateProbabilities": [
        0.5,
        0.5
      ],
      "instantiationVariableTypeMap": {
        "message": "message"
      },
      "outputVariableTypeMap": {
        "creator": "person"
      },      
      "baseProbabilityTemplate": 0.25,
      "minRefinementLength": 2,
      "maxRefinementLength": 4,
      "maxLogits": 10,
      "templateFilePath": {
        "@id": "urn:variables:query-templates:interactive-short-5.sparql",
        "@type": "Variable"
      },
      "refinementPatternsFilePath": {
        "@id": "urn:variables:query-refinements:interactive-short-5.json",
        "@type": "Variable"
      },
      "variables": [
        {
          "@type": "VariableTemplateNamedNode",
          "name": "message",
          "valueTransformers": [
            {
              "@id": "urn:sparql-query-parameter-instantiator:valueTransformerCsvMapActivities"
            }
          ],
          "substitutionProvider": {
            "@id": "urn:sparql-query-parameter-instantiator:substitutionProviderMessageProbability",
            "@type": "SubstitutionProviderUnionProbabilities",
            "substitutionProviders": [
              {
                "@id": "urn:sparql-query-parameter-instantiator:substitutionProviderCommentProbability",
                "@type": "SubstitutionProviderCsvSimilarityBasedProbability",
                "csvFilePath": "out-fragments/parameters-comments.csv",
                "csvFilePathSimilarities": "out-enhanced/parameters-similarities-comments.csv",
                "columnName": "subject",
                "columnNameSimilaritySubject": "person",
                "separator": ";"
              },
              {
                "@id": "urn:sparql-query-parameter-instantiator:substitutionProviderPostProbability",
                "@type": "SubstitutionProviderCsvSimilarityBasedProbability",
                "csvFilePath": "out-fragments/parameters-posts.csv",
                "csvFilePathSimilarities": "out-enhanced/parameters-similarities-posts.csv",
                "columnName": "subject",
                "columnNameSimilaritySubject": "person",
                "separator": ";"
              }
            ]
          }
        }
      ],
      "iriTransformer": {
        "@id": "urn:sparql-query-parameter-instantiator:valueTransformerIri"
      }
    }
  ]
}